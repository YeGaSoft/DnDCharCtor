@attribute [Route(Routes.EditCharacter)]
@page "/editCharacter/{id}"

@inject EditCharacterViewModel ViewModel
@inject IHybridCacheService HybridCacheService

<CascadingValue Name="@CascadeValueNames.DataContext" Value="@ViewModel">

    <PersonalityCard PersonalityExpression="@(() => ViewModel.CharacterViewModelToEdit.PersonalityViewModel)" />

    <FluentButton OnClick="SaveChanges">Save</FluentButton>

</CascadingValue>
